      <menu id="contact-chat-multi" label="_('Send Message...')" hidden="true">
        <menupopup/>
      </menu>
      <menu id="contact-invite" label="Invite to" hidden="true">
        <menupopup/>
      </menu>

      <menuseparator/>

      <menuitem label="_('Remove Contact...')" oncommand="menuModel.onRemove()"/>
